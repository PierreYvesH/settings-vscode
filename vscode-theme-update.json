        "workbench.tree.indent"          : 14,
        "workbench.iconTheme"            : "material-icon-theme",
        "workbench.activityBar.visible"  : true,
        "editor.tokenColorCustomizations": {
            "[Monokai]": {
                "textMateRules": 
                [
                    {
                        "name"    : "decorator",
                        "scope"   : "entity.name.function.decorator.python,meta.function.decorator.python, meta.function-call.arguments.python",
                        "settings": {
                            "foreground": "#f7adeb"
                        }
                    },
                {
                    "name"    : "Strings",
                    "scope"   : "string.character.escape,string.quoted.single,string.quoted.double,string.type.declaration.annotation,string.template.quoted.punctuation.tag, storage.type, support.type",
                    "settings": {
                        "foreground": "#F0B67F"
                    }
                },
                {
                    "name"    : "Doc",
                    "scope"   : "comment,string.quoted,comment.block.documentation",
                    "settings": {
                        "foreground": "#b49e7b"
                    }
                },
                {
                    "name"    : "Exceptions",
                    "scope"   : "support.type.exception.python",
                    "settings": {
                        "foreground": "#00b7ff"
                    }
                },
                
                {
                    "name"    : "import",
                    "scope"   : "keyword.control.import",
                    "settings": 
                    {
                        "foreground": "#D33682"
                    },
                },
                {
                    "name"    : "Storage type, class def",
                    "scope"   : "storage,storage.type.class,storage.type.function,storage.modifier,storage.arrow,",
                    "settings": {
                      "foreground": "#8458f3"
                    }
                },

                {
                    "name"    : "Types, Class Types",
                    "scope"   : "entity.name.type,meta.return.type,meta.type.annotation,meta.type.parameters,support.type.primitive, support.type.python",
                    "settings": {
                      "foreground": "#d64161"
                    }
                },
                {
                    "name"    : "Inherited class",
                    "scope"   : "entity.other.inherited-class",
                    "settings": {
                      "foreground": "#ff7b25"
                    }
                },
                {
                    "name"    : "Function",
                    "scope"   : "entity.name.function,other.name.function,entity.name, support.function.builtin.python, support.function.magic.python,meta.function-call.generic.python",
                    "settings": {
                      "foreground": "#FE5F55"
                    }
                },
                {
                    "name"    : "Number",
                    "scope"   : "constant.numeric, constant.other.caps.python",
                    "settings": {
                      "foreground": "#7A9EC2"
                    }
                },
                {
                    "name"    : "args",
                    "scope"   : "meta.function-call.arguments.python",
                    "settings": {
                      "foreground": "#ffffff"
                    }
                    
                },

                {
                    "name"    : "Function arguments",
                    "scope"   : "meta.parameter, variable.language",
                    "settings": {
                        "foreground": "#ff04dd",
                        "fontStyle" : "italic"
                    }
                },
                {
                    "name"    : "Json arguments",
                    "scope"   : "",
                    "settings": {
                        "foreground": "#ff04dd",
                        "fontStyle" : "italic"
                    }
                },
            ]    
        }
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "svn.enableProposedApi": "none",
    "python.jediEnabled": true,
    "git.autofetch": true,
}
